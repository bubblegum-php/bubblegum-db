{
    "name": "bubblegum-php/bubblegum-db",
    "description": "Database core for BUBBLEGUM.",
    "keywords": ["bubblegum", "bubblegum-db", "bubblegum-database"],
    "homepage": "https://github.com/bubblegum-php/bubblegum-db",
    "license": "MIT",
    "authors": [
        {
            "name": "LankryF",
            "email": "lankrf@gmail.com"
        }
    ],
    "support": {
        "source": "https://github.com/bubblegum-php/bubblegum-db"
    },
    "require": {
        "php": "^8.0",
        "ext-pdo": "*"
    },
    "require-dev": {
        "bubblegum-php/bubblegum-core": "@dev",
        "bubblegum-php/bubblegum-environment": "@dev"
    },
    "autoload": {
        "psr-4": {
            "Bubblegum\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/bubblegum-php/bubblegum-core.git"
        },
        {
            "type": "git",
            "url": "https://github.com/bubblegum-php/bubblegum-environment.git"
        }
  ]
}
